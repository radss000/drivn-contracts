{
  "name": "express-template",
  "scripts": {
    "dev:env": "yarn build && concurrently 'tsc -w' 'node-dev --respawn dist/app.js'",
    "dev": "env-cmd -f ./envs/.env.local yarn dev:env",
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json && tsc-alias",
    "serve": "env-cmd -f ./envs/.env.local node dist/app.js",
    "type": "tsc -p . --noEmit",
    "init:env": "./scripts/copy-env.sh",
    "format": "prettier --write \"./**/*.{ts,tsx,vue}\"",
    "lint": "prettier --check \"./**/*.{ts,tsx,vue}\"",
    "contracts:custom": "yarn run typechain --target=ethers-v5 './src/.contracts/custom/abis/*' --out-dir ./src/.contracts/custom/typechain && node ./scripts/gen-abis.js"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@gotbit/evm": "^1.1.12",
    "@gotbit/evm-node": "^3.0.1",
    "@gotbit/evm-wallets": "^3.0.2",
    "axios": "^1.3.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "5",
    "ethers-multicall": "^0.2.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-automatic-routes": "^1.1.0",
    "helmet": "^5.0.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "vort": "^0.1.17",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^17.0.31",
    "@types/supertest": "^2.0.12",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "env-cmd": "^10.1.0",
    "jest": "^29.0.3",
    "mocha": "^10.0.0",
    "node-dev": "^7.4.3",
    "prettier": "^2.8.7",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "typechain": "^8.1.1",
    "typescript": "^5.0.2"
  }
}
